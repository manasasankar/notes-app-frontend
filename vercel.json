{
  "version": 2,
  "builds": [
    {
      "src": "frontend/notes-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/notes-app/build" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/notes-app/build/$1"  
    }
  ]
}
