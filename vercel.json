{
    "version": 2,
    "builds": [
      {
        "src": "frontend/notes-app/package.json",
        "use": "@vercel/node",
        "config": {
          "outputDirectory": "frontend/notes-app/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/notes-app/dist/$1"
      }
    ]
  }
  